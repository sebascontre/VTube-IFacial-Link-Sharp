[
  {
    "name": "FacePositionX",
    "script": "return capturedData.Tracking.Head.Position.X * FACE_POSITION_X_RATIO"
  },
  {
    "name": "FacePositionY",
    "script": "return capturedData.Tracking.Head.Position.Y * FACE_POSITION_Y_RATIO"
  },
  {
    "name": "FacePositionZ",
    "script": "return -capturedData.Tracking.Head.Position.Z * FACE_POSITION_Z_RATIO"
  },
  {
    "name": "FaceAngleX",
    "script": "return capturedData.Tracking.Head.Rotation.Y * FACE_ANGLE_X_RATIO"
  },
  {
    "name": "FaceAngleY",
    "script": "return -capturedData.Tracking.Head.Rotation.X * FACE_ANGLE_Y_RATIO"
  },
  {
    "name": "FaceAngleZ",
    "script": "return -capturedData.Tracking.Head.Rotation.Z * FACE_ANGLE_Z_RATIO"
  },
  {
    "name": "MouthSmile",
    "script": "return (\r  (\r    math.max(capturedData.BlendShapes.MouthSmileLeft + capturedData.BlendShapes.MouthSmileRight - 0.2, 0)  -- mouth smile (pos*2)\r    - math.pow(math.max(capturedData.BlendShapes.MouthShrugLower - 0.4, 0), 1) * 1  -- mouth shrug (neg*1) (threshold: 0.4)\r    - math.pow(math.max(\r        (capturedData.BlendShapes.BrowDownLeft + capturedData.BlendShapes.BrowDownRight) / 2 - 0.3 + \r        (0.08 + ((capturedData.BlendShapes.JawOpen - capturedData.BlendShapes.MouthClose) * 0.15))\r        , 0), 0.4) * 1.5  -- brow low (neg*1.5) (threshold: 0.08 + mouth_open_factor)\r  ) * MOUTH_SMILE_RATIO  -- ratio\r) / 2 + 0.5 "
  },
  {
    "name": "MouthOpen",
    "script": "return (capturedData.BlendShapes.JawOpen - capturedData.BlendShapes.MouthClose) * MOUTH_OPEN_RATIO"
  },
  {
    "name": "Brows",
    "script": "return capturedData.BlendShapes.BrowInnerUp * BROWS_RATIO"
  },
  {
    "name": "TongueOut",
    "script": "if (capturedData.BlendShapes.TongueOut < TONGUE_OUT_RATIO) then\r  return 0\relse\r  return 1\rend"
  },
  {
    "name": "EyeOpenLeft",
    "script": "return (1 - capturedData.BlendShapes.EyeBlinkLeft) * EYE_OPEN_RATIO - (EYE_OPEN_RATIO - 1)"
  },
  {
    "name": "EyeOpenRight",
    "script": "return (1 - capturedData.BlendShapes.EyeBlinkRight) * EYE_OPEN_RATIO - (EYE_OPEN_RATIO - 1)"
  },
  {
    "name": "EyeLeftX",
    "script": "return (capturedData.BlendShapes.EyeLookInLeft - capturedData.BlendShapes.EyeLookOutLeft) * EYE_ROTATION_RATIO"
  },
  {
    "name": "EyeLeftY",
    "script": "return (capturedData.BlendShapes.EyeLookUpLeft - capturedData.BlendShapes.EyeLookDownLeft) * EYE_ROTATION_RATIO"
  },
  {
    "name": "EyeRightX",
    "script": "return (capturedData.BlendShapes.EyeLookOutRight - capturedData.BlendShapes.EyeLookInRight) * EYE_ROTATION_RATIO"
  },
  {
    "name": "EyeRightY",
    "script": "return (capturedData.BlendShapes.EyeLookUpRight - capturedData.BlendShapes.EyeLookDownRight) * EYE_ROTATION_RATIO"
  },
  {
    "name": "CheekPuff",
    "script": "return capturedData.BlendShapes.CheekPuff * CHEEK_PUFF_RATIO"
  },
  {
    "name": "FaceAngry",
    "script": "if ((capturedData.BlendShapes.MouthRollLower * capturedData.BlendShapes.MouthShrugLower) < FACE_ANGRY_RATIO) then\r  return 0\relse\r  return 1\rend"
  },
  {
    "name": "BrowLeftY",
    "script": "return ((capturedData.BlendShapes.BrowOuterUpLeft - capturedData.BlendShapes.BrowDownLeft) * BROW_LEFT_Y_RATIO + 1) / 2"
  },
  {
    "name": "BrowRightY",
    "script": "return ((capturedData.BlendShapes.BrowOuterUpRight - capturedData.BlendShapes.BrowDownRight) * BROW_RIGHT_Y_RATIO + 1) / 2"
  },
  {
    "name": "MouthX",
    "script": "return (capturedData.BlendShapes.MouthLeft - capturedData.BlendShapes.MouthRight) * MOUTH_X_RATIO"
  },

